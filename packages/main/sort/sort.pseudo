function bubble_sort(arr)
    n = arr.length()
    loop i = 0 to n-2
        loop j = 0 to n-i-2
            if arr[j] > arr[j+1] then
                temp = arr[j]
                arr[j] = arr[j+1]
                arr[j+1] = temp
            end if
        end loop
    end loop
    return arr
end function